{
  "manifest_version": 2,

  "name": "A space launch calendar notification extension",
  "description": "This extension shows upcoming launches",
  "version": "0.1",

  "browser_action": {
    "default_icon": "rocket-16.png",
    "default_popup": "popup.html",
    "default_title": "Show launches!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
    ],
  "icons": {
      "16": "rocket-16.png",
      "48": "rocket-48.png",
      "128": "rocket-128.png"
  }
}
