{
  "manifest_version": 2,

  "name": "A space launch schedule overview",
  "description": "This extension shows 5 upcoming launches",
  "version": "0.2",

  "browser_action": {
    "default_icon": "rocket-16.png",
    "default_popup": "popup.html",
    "default_title": "Show launches!"
  },
  "permissions": [
      "http://launchlibrary.net/",
      "notifications"
  ],
  "icons": {
      "16": "rocket-16.png",
      "48": "rocket-48.png",
      "128": "rocket-128.png"
  }
}
