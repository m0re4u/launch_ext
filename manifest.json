{
  "manifest_version": 2,

  "name": "A space launch calendar notification extension",
  "description": "This extension shows upcoming launches",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
